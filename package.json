{
  "name": "datapilot",
  "version": "1.1.1",
  "description": "🛩️ Revolutionary self-contained CSV analysis engine with interactive UI, 5 specialized analysis modes, and AI-ready insights. Zero installation, zero dependencies, maximum insights.",
  "main": "bin/datapilot.js",
  "bin": {
    "datapilot": "./dist/datapilot.js"
  },
  "type": "module",
  "files": [
    "dist/",
    "frontend/dist/index.html",
    "frontend/dist/vite.svg",
    "frontend/dist/assets/",
    "README.md"
  ],
  "scripts": {
    "test": "node tests/run_tests.js",
    "test:tui": "node tests/run_tui_tests.js",
    "test:tui:auto": "node tests/automated_navigation_test.js",
    "test:tui:windows": "tests\\tui_test_windows.bat",
    "test:tui:powershell": "powershell -ExecutionPolicy Bypass -File tests/Test-DataPilotTUI.ps1",
    "test:all": "npm run test && npm run test:tui",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:portable": "node scripts/build/create-portable-bundle.js",
    "clean:portable": "rm -rf datapilot-portable-*",
    "build:frontend": "cd frontend && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "install:frontend": "cd frontend && npm install",
    "ui": "node bin/datapilot.js ui",
    "wui": "node bin/datapilot.js wui",
    "tui": "node bin/datapilot.js tui",
    "dev:ui": "npm run build:frontend && npm run ui",
    "prepublishOnly": "npm run build:frontend && npm run build"
  },
  "keywords": [
    "csv",
    "data-analysis",
    "cli",
    "tui",
    "interactive",
    "eda",
    "exploratory-data-analysis",
    "data-quality",
    "data-integrity",
    "visualization",
    "data-engineering",
    "llm",
    "ai-ready",
    "chatgpt",
    "claude",
    "self-contained",
    "zero-dependencies",
    "data-science",
    "statistics",
    "correlation",
    "outliers",
    "data-validation",
    "business-intelligence",
    "insights",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chardet": "^0.7.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "enquirer": "^2.4.1",
    "express": "^4.21.2",
    "fuzzyset.js": "^1.0.7",
    "glob": "^11.0.2",
    "js-yaml": "^4.1.0",
    "jstat": "^1.9.6",
    "libphonenumber-js": "^1.12.8",
    "ml-cart": "^2.1.1",
    "ml-kmeans": "^6.0.0",
    "multer": "^2.0.0",
    "nanospinner": "^1.2.2",
    "open": "^10.1.2",
    "ora": "^7.0.0",
    "regression": "^2.0.1",
    "simple-statistics": "^7.8.8",
    "validator": "^13.15.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.41.1",
    "rollup-plugin-preserve-shebang": "^1.0.1"
  }
}
